{
  "compilerOptions": {
    "baseUrl": ".",
    "module": "commonjs",
    "target": "es6",
    "experimentalDecorators": true,
    "declaration": false,
    "noImplicitAny": false,
    "sourceMap": true,
    "inlineSources": true,
    "removeComments": true,
    "allowJs": true,
    "emitDecoratorMetadata": true,
    "resolveJsonModule": true
  },
  "include": ["./"],
  "exclude": ["node_modules"]
}
